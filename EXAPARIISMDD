n = int (input("Cantidad de numeros: "))
suma = 0
while n >= 0:
    num = int(input("Numero: "))
    suma = suma + num
    n = n - 1
    print ("La suma total es",suma)

m = int(input("Cantidad de numeros que desea"))
multiplicar = 0
while m >= 0:
    num = int(input("Numeros: "))
    multiplicar = multiplicar * num
    m = m -1
    print("El total es",multiplicar)
